<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <title>{% block title %}Wize Guys{% endblock %}</title>

    <!-- Bootstrap -->
    {% block stylesheets %}
    <link href="{{ asset('css/plugins.css') }}" rel="stylesheet">
    <link href="{{ asset('css/bootstrap-datepicker.min.css') }}" rel="stylesheet">
    <link href="{{ asset('css/bootstrap-clockpicker.min.css') }}" rel="stylesheet">
    <link href="{{ asset('css/lightbox.min.css') }}" rel="stylesheet">
    <!--slider revolution-->
    <link rel="stylesheet" type="text/css" href="{{ asset('css/settings.css') }}">
        <link rel="stylesheet" type="text/css" href="{{ asset('css/navigation.css') }}">
    <link rel="stylesheet" type="text/css" href="{{ asset('css/layers.css') }}">
    <!--main css file-->
    <link href="{{ asset('css/style.css') }}" rel="stylesheet">



    <link rel="stylesheet" type="text/css" href="http://code.ionicframework.com/ionicons/2.0.1/css/ionicons.min.css">

    <!-- HTML5 shim and Respond.js for IE8 support of HTML5 elements and media queries -->
    <!-- WARNING: Respond.js doesn't work if you view the page via file:// -->
    {% endblock %}

</head>
<body data-spy="scroll" data-offset="62" onload="myFunction()">
<div id="preloader">
    <div id="preloader-inner"></div>
</div><!--/preloader-->
<!--===============main navigation + top bar==========================-->
<header class="header header-top-transparent">

    <!--top bar-->
    <div class="top-bar">
        <div class="container">
            <div class="row">
                <div class="col-sm-6 hidden-sm-down">
                    <ul class="list-inline">
                        <li class="list-inline-item"><a href="javascript:void(0)"><i class="ion-ios-telephone"></i> (201) 343-1000</a></li>

                        {% if app.session.get('loginUserId') != "" %}
                        <li class="list-inline-item"  ><a href="profile-info" target="_blank"> &nbsp; &nbsp; <i class="ion-pizza"></i>Hi,   {{ app.session.get('loginUserId') }}</a> </li>
                        <li class="list-inline-item" > <a href="{{ path('logout')}}" target="_blank" >  <u> Not  {{ app.session.get('loginUserId') }}? Sign out</u></a></li>
                                {% else %}
                        <li class="list-inline-item" ><a href="login-register"  target="_blank"><i class="ion-pizza"></i>  SIGN IN & EARN REWARDS</a></li>{% endif %}

                      <!--  <li class="list-inline-item"><a href="javascript:void(0)"><i class="ion-email"></i> support@resto.com</a></li>-->
                    </ul>
                </div>
                <div class="col-sm-6 text-right">
                    <ul class="list-inline level-2">
                        <li class="btn btn-light"><a href="https://www.opendining.net/menu/4f69da0e000d97fd5c0001bf" target="_blank" class=" waves-float waves-button">Order Online</a></li> <h5 style="color:white; display: inline "></h5>
                    </ul>
                </div>
            </div>
        </div>
    </div>
    <!--end top bar-->
    <!--main navigation-->
    <nav class="navbar navbar-light navbar-expand-lg navbar-static-top sticky-header">
        <div class="container">
            <div class="navbar-header">
                <button type="button" aria-label="Toggle navigation" class="navbar-toggler navbar-toggler-right collapsed" data-toggle="collapse" data-target="#navbarNavDropdown" aria-expanded="false" aria-controls="navbarNavDropdown">
                    <span class="navbar-toggler-icon"></span>
                </button>
                <a class="navbar-brand" href="hackensack">

                    <img src="{{ asset('images/logo-light.png') }}" alt="logo" class="logo-default">
                    <img src="{{ asset('images/logo-dark.png') }}" alt="logo" class="logo-scroll">

                </a>
            </div>
            <div class="collapse navbar-collapse" id="navbarNavDropdown">
                <ul class="navbar-nav ml-auto scroll-to">
                    <li class="nav-item"><a href="hackensack#home" class="nav-link active">home</a></li>

                    <li class="nav-item"><a href="hackensack#menu" class="nav-link">Menu</a></li>
                    <li class="nav-item"><a href="hackensack#reservation" class="nav-link">Reservation</a></li>
                    <!-- <li class="nav-item"><a href="#blog" class="nav-link">Blog</a></li>-->
                    <li class="nav-item"><a href="hackensack#contact" class="nav-link">Contact</a></li>
                    <li class="nav-item"><a href="about" class="nav-link">About</a></li>
                </ul>
            </div><!--/.nav-collapse -->
        </div>
    </nav>
</header>


<main role="main">
    <div class="row">
        <div class="col-md-12">
            {% for flash_message in app.session.flashbag.get('notice') %}
                <div class="alert alert-success"> {{flash_message}}</div>
            {% endfor %}


            {% for flash_message in app.session.flashbag.get('error') %}
                <div class="alert alert-danger"> {{flash_message}}</div>
            {% endfor %}
            {% block body%}{% endblock %}
        </div>
        <a class="scroll-to-top rounded" href="#header-container">
            <i class="fa fa-angle-up"></i>
        </a>
    </div>


</main><!-- /.container -->


<footer class="footer">
    <div class="container">
        <div class="row">
            <div class="col-lg-3 col-md-6 margin-b-30">
                <h3>Opening hours</h3>
                <p><span class="text-color">Mon-Thu :</span> 11:00Am - 10:00PM</p>
                <p><span class="text-color">Sat-Fri :</span> 11:00Am - 11:00PM</p>
                <p><span class="text-color">Sunday :</span> 12:00PM - 9:00PM</p>
            </div>
            <div class="col-lg-3 col-md-6 margin-b-30">
                <h3>Contact Us</h3>
                <p>
                    380 W Pleasantview Ave, Hackensack, NJ 07601
                </p>
                <p >(201) 343-1000</p>
                <p>info@wizeguys.com</p>
            </div>
            <div class="col-lg-3 col-md-6 margin-b-30">
                <h3>Nav Menu</h3>
                <ul class="navbar-nav ml-auto scroll-to">

                    <li class="nav-item"><a href="hackensack#menu" class="nav-link">Menu</a></li>
                    <li class="nav-item"><a href="hackensack#reservation" class="nav-link">Reservation</a></li>
                    <!-- <li class="nav-item"><a href="#blog" class="nav-link">Blog</a></li>-->
                    <li class="nav-item"><a href="hackensack#contact" class="nav-link">Contact</a></li>
                    <li class="nav-item"><a href="about" class="nav-link">About</a></li>
                </ul>
            </div>
        </div>
        <hr>
        <div class="row">
            <div class="col-md-12 text-center">
                <ul class="list-inline f-social">
                    <li class="list-inline-item"><a href="https://www.facebook.com/wizeguyspizzeria/"><i class="ion-social-facebook"></i></a></li>
                    <li class="list-inline-item"><a href="https://www.instagram.com/wizeguyspizza/?hl=en"><i class="ion-social-instagram"></i></a></li>
                </ul>
                <span>&copy; Copyright 2018. Wize Guys</span>
            </div>
        </div>
    </div>
</footer>



<!--back to top-->
<a href="#" class="scrollToTop"><i class="ion-android-arrow-up"></i></a>
<!--back to top end-->



<!-- jQuery plugins-->
<script src="{{ asset('js/plugins.js') }}"></script>
<script src="{{ asset('js/isotope.pkgd.min.js') }}" type="text/javascript"></script>
<script src=".{{ asset('js/imagesloaded.pkgd.min.js') }}" type="text/javascript"></script>
<script src="{{ asset('js/bootstrap-clockpicker.min.js') }}"></script>
<script src="{{ asset('js/bootstrap-datepicker.min.js') }}"></script>
<script src="{{ asset('js/jquery.validate.min.js') }}"></script>
<script src="{{ asset('js/reservation-custom.js') }}"></script>
<!--revolution slider-->
<script type="text/javascript" src="{{ asset('js/jquery.themepunch.tools.min.js') }}"></script>
<script type="text/javascript" src="{{ asset('js/jquery.themepunch.revolution.min.js') }}"></script>
<script type="text/javascript" src="{{ asset('js/revolution.extension.video.min.js') }}"></script>
<script type="text/javascript" src="{{ asset('js/revolution.extension.slideanims.min.js') }}"></script>
<script type="text/javascript" src="{{ asset('js/revolution.extension.layeranimation.min.js') }}"></script>
<script type="text/javascript" src="{{ asset('js/revolution.extension.navigation.min.js') }}"></script>
<script type="text/javascript" src="{{ asset('js/revolution.extension.parallax.min.js') }}"></script>
<script src="{{ asset('js/onepage-custom.js') }}" type="text/javascript"></script>
<script src="{{ asset('js/lightbox.min.js') }}"></script>
<script src="{{ asset('js/contact.js') }}" type="text/javascript"></script>
<!-- Google Maps -->
<script async defer
        src="https://maps.googleapis.com/maps/api/js?key=AIzaSyCRjd2tW7v_fxKxXz1QqNsID15wO0dEAiw&callback=initMap">
</script>
<script src="{{ asset('js/jquery.gmaps.min.js') }}"></script>

<!--[if lt IE 9]>
<script src="https://oss.maxcdn.com/html5shiv/3.7.2/html5shiv.min.js"></script>
<script src="https://oss.maxcdn.com/respond/1.4.2/respond.min.js"></script>


<![endif]-->
{% block javascripts %}
{% endblock %}
</body>
</html>